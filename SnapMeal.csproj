<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net8.0-android;net8.0-ios;net8.0-maccatalyst</TargetFrameworks>
		<TargetFrameworks Condition="$([MSBuild]::IsOSPlatform('windows'))">$(TargetFrameworks);net8.0-windows10.0.19041.0</TargetFrameworks>
		<!-- Uncomment to also build the tizen app. You will need to install tizen by following this: https://github.com/Samsung/Tizen.NET -->
		<!-- <TargetFrameworks>$(TargetFrameworks);net8.0-tizen</TargetFrameworks> -->

		<!-- Note for MacCatalyst:
		The default runtime is maccatalyst-x64, except in Release config, in which case the default is maccatalyst-x64;maccatalyst-arm64.
		When specifying both architectures, use the plural <RuntimeIdentifiers> instead of the singular <RuntimeIdentifier>.
		The Mac App Store will NOT accept apps with ONLY maccatalyst-arm64 indicated;
		either BOTH runtimes must be indicated or ONLY macatalyst-x64. -->
		<!-- For example: <RuntimeIdentifiers>maccatalyst-x64;maccatalyst-arm64</RuntimeIdentifiers> -->

		<OutputType>Exe</OutputType>
		<RootNamespace>SnapMeal</RootNamespace>
		<UseMaui>true</UseMaui>
		<SingleProject>true</SingleProject>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>

		<!-- Display name -->
		<ApplicationTitle>SnapMeal</ApplicationTitle>

		<!-- App Identifier -->
		<ApplicationId>com.companyname.snapmeal</ApplicationId>

		<!-- Versions -->
		<ApplicationDisplayVersion>1.0</ApplicationDisplayVersion>
		<ApplicationVersion>1</ApplicationVersion>

		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'ios'">11.0</SupportedOSPlatformVersion>
		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'maccatalyst'">13.1</SupportedOSPlatformVersion>
		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'android'">21.0</SupportedOSPlatformVersion>
		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'windows'">10.0.17763.0</SupportedOSPlatformVersion>
		<TargetPlatformMinVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'windows'">10.0.17763.0</TargetPlatformMinVersion>
		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'tizen'">6.5</SupportedOSPlatformVersion>
	</PropertyGroup>

	<ItemGroup>
		<!-- App Icon -->
		<MauiIcon Include="Resources\AppIcon\appicon.svg" ForegroundFile="Resources\AppIcon\appiconfg.svg" Color="#512BD4" />

		<!-- Splash Screen -->
		<MauiSplashScreen Include="Resources\Splash\splash.svg" Color="#512BD4" BaseSize="128,128" />

		<!-- Images -->
		<MauiImage Include="Resources\Images\*" />
		<MauiImage Update="Resources\Images\dotnet_bot.png" Resize="True" BaseSize="300,185" />

		<!-- Custom Fonts -->
		<MauiFont Include="Resources\Fonts\*" />

		<!-- Raw Assets (also remove the "Resources\Raw" prefix) -->
		<MauiAsset Include="Resources\Raw\**" LogicalName="%(RecursiveDir)%(Filename)%(Extension)" />
	</ItemGroup>

	<ItemGroup>
	  <Content Include="SnapMeal.App\obj\Debug\net9.0-android\lp\163\jl\.net\__res_name_case_map.txt" />
	  <Content Include="SnapMeal.App\obj\Debug\net9.0-android\lp\165\jl\.net\__res_name_case_map.txt" />
	  <Content Include="SnapMeal.App\obj\Debug\net9.0-android\lp\166\jl\.net\__res_name_case_map.txt" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Maui.Controls" Version="$(MauiVersion)" />
		<PackageReference Include="Microsoft.Maui.Controls.Compatibility" Version="$(MauiVersion)" />
		<PackageReference Include="Microsoft.Extensions.Logging.Debug" Version="8.0.1" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="SnapMeal.API\bin\Debug\net9.0\" />
	  <Folder Include="SnapMeal.API\bin\Release\net9.0\" />
	  <Folder Include="SnapMeal.API\obj\Debug\net9.0\refint\" />
	  <Folder Include="SnapMeal.API\obj\Debug\net9.0\ref\" />
	  <Folder Include="SnapMeal.API\obj\Debug\net9.0\staticwebassets\" />
	  <Folder Include="SnapMeal.API\obj\Release\net9.0\refint\" />
	  <Folder Include="SnapMeal.API\obj\Release\net9.0\ref\" />
	  <Folder Include="SnapMeal.App\bin\Debug\net9.0-android\" />
	  <Folder Include="SnapMeal.App\bin\Debug\net9.0-ios\iossimulator-x64\" />
	  <Folder Include="SnapMeal.App\bin\Debug\net9.0-maccatalyst\maccatalyst-x64\" />
	  <Folder Include="SnapMeal.App\bin\Debug\net9.0-windows10.0.19041.0\win10-x64\" />
	  <Folder Include="SnapMeal.App\obj\Debug\net9.0-ios\iossimulator-x64\refint\" />
	  <Folder Include="SnapMeal.App\obj\Debug\net9.0-ios\iossimulator-x64\ref\" />
	  <Folder Include="SnapMeal.App\obj\Debug\net9.0-maccatalyst\maccatalyst-x64\refint\" />
	  <Folder Include="SnapMeal.App\obj\Debug\net9.0-maccatalyst\maccatalyst-x64\ref\" />
	  <Folder Include="SnapMeal.App\obj\Debug\net9.0-windows10.0.19041.0\win10-x64\intermediatexaml\" />
	  <Folder Include="SnapMeal.App\obj\Debug\net9.0-windows10.0.19041.0\win10-x64\refint\" />
	  <Folder Include="SnapMeal.App\obj\Debug\net9.0-windows10.0.19041.0\win10-x64\ref\" />
	  <Folder Include="SnapMeal.Tests\bin\Debug\net9.0\" />
	  <Folder Include="SnapMeal.Tests\bin\Release\net9.0\" />
	  <Folder Include="SnapMeal.Tests\obj\Debug\net9.0\refint\" />
	  <Folder Include="SnapMeal.Tests\obj\Debug\net9.0\ref\" />
	  <Folder Include="SnapMeal.Tests\obj\Release\net9.0\refint\" />
	  <Folder Include="SnapMeal.Tests\obj\Release\net9.0\ref\" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="SnapMeal.API\SnapMeal.API.csproj" />
	  <None Include="SnapMeal.API\SnapMeal.API.csproj.user" />
	  <None Include="SnapMeal.App\SnapMeal.App.csproj" />
	  <None Include="SnapMeal.Tests\SnapMeal.Tests.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <MauiXaml Update="SnapMeal.App\App.xaml">
	    <Generator>MSBuild:Compile</Generator>
	  </MauiXaml>
	  <MauiXaml Update="SnapMeal.App\AppShell.xaml">
	    <Generator>MSBuild:Compile</Generator>
	  </MauiXaml>
	  <MauiXaml Update="SnapMeal.App\MainPage.xaml">
	    <Generator>MSBuild:Compile</Generator>
	  </MauiXaml>
	  <MauiXaml Update="SnapMeal.App\Platforms\Windows\App.xaml">
	    <Generator>MSBuild:Compile</Generator>
	  </MauiXaml>
	  <MauiXaml Update="SnapMeal.App\Resources\Styles\Colors.xaml">
	    <Generator>MSBuild:Compile</Generator>
	  </MauiXaml>
	  <MauiXaml Update="SnapMeal.App\Resources\Styles\Styles.xaml">
	    <Generator>MSBuild:Compile</Generator>
	  </MauiXaml>
	  <MauiXaml Update="SnapMeal.App\Views\DashboardPage.xaml">
	    <Generator>MSBuild:Compile</Generator>
	  </MauiXaml>
	  <MauiXaml Update="SnapMeal.App\Views\LoginPage.xaml">
	    <Generator>MSBuild:Compile</Generator>
	  </MauiXaml>
	  <MauiXaml Update="SnapMeal.App\Views\RecipeDetailPage.xaml">
	    <Generator>MSBuild:Compile</Generator>
	  </MauiXaml>
	</ItemGroup>

</Project>
